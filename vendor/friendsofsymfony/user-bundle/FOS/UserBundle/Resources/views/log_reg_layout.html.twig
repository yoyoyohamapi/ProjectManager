{% extends "CSCVAppBundle:Layout:layout.html.twig" %}
{% block stylesheets %}
    <link href=" {{ asset('bundles/cscvapp/css/1.0/account.css') }}" rel="stylesheet" type="text/css"/>
{% endblock %}

{% block content %}
    <div class="ui one column stackable center aligned page grid ">
        <div class="row">
            <img src="{{ asset('bundles/cscvapp/images/1.0/account/title.png') }}" class="account_title"/>
        </div>
        <div class="column six wide account_form_container">
            {% block fos_user_content %}
            {% endblock %}
        </div>
    </div>
{% endblock %}
{% block scripts %}
    {{ parent() }}
{% endblock %}